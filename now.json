{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "page?uid=$1"
    }
  ]
}
