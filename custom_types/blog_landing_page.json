{
  "Main": {
    "uid": {
      "type": "UID",
      "config": {
        "label": "UID",
        "placeholder": "e.g. blog-landing-page"
      }
    },
    "page_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1",
        "label": "Blog Title",
        "placeholder": "e.g. Blog"
      }
    },
    "featured_post": {
      "type": "Link",
      "config": {
        "select": "document",
        "customtypes": ["blog_post"],
        "label": "Featured Post",
        "placeholder": "Choose a featured blog post"
      }
    },
    "body": {
      "type": "Slices",
      "fieldset": "Slice zone",
      "config": {
        "choices": {}
      }
    }
  },
  "SEO": {
    "meta_title": {
      "type": "StructuredText",
      "config": {
        "single": "heading1",
        "label": "meta_title"
      }
    },
    "meta_desc": {
      "type": "StructuredText",
      "config": {
        "single": "paragraph",
        "label": "meta_desc"
      }
    },
    "og_image": {
      "type": "Image",
      "config": {
        "constraint": {
          "width": 1200,
          "height": 630
        },
        "thumbnails": [],
        "label": "og_image"
      }
    }
  }
}
